{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "run:docs": "turbo run dev --filter=docs",
    "run:docusaurus": "turbo run dev --filter=docusaurus",
    "run:expo-demo": "turbo run dev --filter=expo-demo"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "tsconfig": "*",
    "turbo": "latest"
  },
  "name": "nativeflare",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
